// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model products {
  id          String @id @default(dbgenerated("gen_random_uuid()"))
  title       String
  description String @db.VarChar(500)
}

model warehouses {
  id          String @id @default(dbgenerated("gen_random_uuid()"))
  location    String
  description String @db.VarChar(500)
}
